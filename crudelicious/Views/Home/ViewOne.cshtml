    @model Dish
    
    <h1>@ViewBag.Dish.Name</h1>
    <h4>@ViewBag.Dish.Chef</h4>
    <hr>
    <p>@ViewBag.Dish.Description</p>
    <br>
    <p>Calories: @ViewBag.Dish.Calories</p>
    <p>Tastiness: @ViewBag.Dish.Tastiness</p>

    <a href="/delete/@ViewBag.Dish.DishID" class="btn btn-danger">Delete</a>
    <a href="/edit/@ViewBag.Dish.DishID" class="btn btn-warning">Edit</a>